<ul id="actionlog_associated_attrs">
    <h3>ActionLog Associated Attributes</h3>
    <% @associated_attrs.where(endpoint_id: 1).each do |associated_attr| %>
    <li><%= associated_attr.attr.attr_title %></li>
    <li><%= associated_attr.ui_info %></li>
    <li><%= associated_attr.product.product_title %></li>
    <% end %>
</ul>