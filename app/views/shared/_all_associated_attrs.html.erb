<div class="endpoint_title">
    <h1>#{"endpoint title"}</h1>
</div>

<div class="associated_attrs">

    <div class="product_association" id="product_id">

        <div class="associated_product">
            <%= image_tag("product.png") %>
            <h3>product</h3>
        </div>

        <div class="product_associated_attrs">
            <% @associated_attrs.joins(:endpoint).where(endpoints: { endpoint_title: "endpoint" }).each do |associated_attr| %>
                <% if associated_attr.ui_info['product'].present? %>
                    <ul>
                        <li><h5><%= associated_attr.attr.attr_title %></h5></li>
                        <li><hr></li>
                        <li><h6><%= associated_attr.ui_info['product'] %></h6></li>    
                    </ul>
                <% end %>
            <% end %>
        </div>

    </div>
    
<%= render 'shared/nav' %>