<h1>Associated Attributes</h1>
<%= link_to 'New Associated Attribute', new_associated_attr_path %>
<table>
  <thead>
    <tr>
      <th>Request Type</th>
      <th>UI Info</th>
      <th>Associated Endpoints</th>
      <th>Attr Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @associated_attrs.each do |attr| %>
      <tr>
        <td><%= attr.request_type %></td>
        <td><%= attr.ui_info %></td>
        <td><%= attr.associated_endpoints %></td>
        <td><%= attr.attr_title %></td>
        <td><%= link_to 'Show', attr %></td>
        <td><%= link_to 'Edit', edit_associated_attr_path(attr) %></td>
        <td><%= link_to 'Destroy', attr, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
